<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <record id="res_users_groups_view_tree" model="ir.ui.view">
        <field name="name">res.users.groups.view.tree</field>
        <field name="model">res.users.groups</field>
        <field name="arch" type="xml">
            <tree string="Res Users Groups" editable="bottom">
                <field name="name" />
                <field name="group_id" />
            </tree>
        </field>
    </record>
    
    <record id="res_users_groups_action" model="ir.actions.act_window">
        <field name="name">Res Users Groups</field>
        <field name="res_model">res.users.groups</field>
        <field name="view_mode">tree</field>
    </record>

</odoo>
